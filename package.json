    "ws": "^7.2.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.22",
    "@types/node": "^12.12.6",
    "jest": "^24.9.0",
    "microbundle": "^0.11.0",
    "ts-jest": "^24.1.0",
    "typescript": "^3.7.2"
  }
